# Class: :Anomaly (Anomaly)

AnnotationAssertion(rdfs:isDefinedBy :Anomaly "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :Anomaly "Anomaly"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :Anomaly "Description of possible unwanted behaviour"@en)
EquivalentClasses(:Anomaly ObjectSomeValuesFrom(<http://www.w3.org/ns/sosa/usedProcedure> <http://www.w3.org/ns/sosa/Procedure>))
EquivalentClasses(:Anomaly ObjectSomeValuesFrom(<http://www.w3.org/ns/ssn/wasOriginatedBy> :fromObservation))
EquivalentClasses(:Anomaly ObjectSomeValuesFrom(<http://www.w3.org/ns/ssn/wasOriginatedBy> :observedProperty))
EquivalentClasses(:Anomaly ObjectSomeValuesFrom(<http://www.w3.org/ns/ssn/wasOriginatedBy> :toObservation))
EquivalentClasses(:Anomaly ObjectSomeValuesFrom(<http://www.w3.org/ns/ssn/wasOriginatedBy> <http://www.w3.org/ns/ssn/Stimulus>))
EquivalentClasses(:Anomaly ObjectExactCardinality(1 <http://www.w3.org/ns/sosa/hasResult> <http://www.w3.org/ns/sosa/resultTime>))
SubClassOf(:Anomaly ObjectIntersectionOf(ObjectSomeValuesFrom(:hasCategory :Category) ObjectSomeValuesFrom(:hasCause :Cause) ObjectSomeValuesFrom(:hasCriticality :Criticality) ObjectSomeValuesFrom(:hasDetection :DetectionMethod) ObjectSomeValuesFrom(:hasDomain :ConsequenceDomain) ObjectSomeValuesFrom(:hasEffect :Effect)))
SubClassOf(:Anomaly ObjectSomeValuesFrom(:hasSubAnomaly :Anomaly))
SubClassOf(:Anomaly ObjectMaxCardinality(1 :hasStatus :Status))

# Class: :Category (Category)

AnnotationAssertion(rdfs:isDefinedBy :Category "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :Category "Category"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :Category "An ConfirmedAnomaly can happen once (transient), periodically (recurrent) or definitively (permanent)."@en)

# Class: :Cause (Cause)

AnnotationAssertion(rdfs:isDefinedBy :Cause "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :Cause "Cause"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :Cause "The cause refers to the description suspected to be the source of the ConfirmedAnomaly"@en)

# Class: :Confirmed (:Confirmed)

SubClassOf(:Confirmed :Status)

# Class: :ConfirmedAnomaly (ConfirmedAnomaly)

AnnotationAssertion(rdfs:isDefinedBy :ConfirmedAnomaly "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :ConfirmedAnomaly "ConfirmedAnomaly"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :ConfirmedAnomaly "ConfirmedAnomaly’s description. Should then express, at least, which part of the system is affected, by what. Ideally, this should be completed with additional information, such as which components observe the ConfirmedAnomaly, or the worth of its management."@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :ConfirmedAnomaly "ConfirmedAnomaly’s description. Should then express, at least, which part of the system is affected, by what. Ideally, this should be completed with additional information, such as which components observe the ConfirmedAnomaly, or the worth of its management."@en)
EquivalentClasses(:ConfirmedAnomaly ObjectAllValuesFrom(:hasStatus :Confirmed))
SubClassOf(:ConfirmedAnomaly :Anomaly)

# Class: :ConsequenceDomain (ConsequenceDomain)

AnnotationAssertion(rdfs:isDefinedBy :ConsequenceDomain "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :ConsequenceDomain "ConsequenceDomain"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :ConsequenceDomain "Each domain relates to a specific kind of consequence of the ConfirmedAnomaly."@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :ConsequenceDomain "Malfunctioning of device or dangerous situation for end user."@en)

# Class: :Criticality (Criticality)

AnnotationAssertion(rdfs:isDefinedBy :Criticality "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :Criticality "Criticality"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :Criticality "It is a time dependent worth attributed to the ConfirmedAnomaly management."@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :Criticality "non-critic ConfirmedAnomaly"@en)

# Class: :DetectionMethod (DetectionMethod)

AnnotationAssertion(rdfs:isDefinedBy :DetectionMethod "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :DetectionMethod "DetectionMethod"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :DetectionMethod "This relies on components’ survey to trigger a warning about abnormalities."@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :DetectionMethod "value > x."@en)

# Class: :Effect (Effect)

AnnotationAssertion(rdfs:isDefinedBy :Effect "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :Effect "Effect"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :Effect "The effect describes one aspect of the observed phenomenon."@en)

# Class: :FailureCause (FailureCause)

AnnotationAssertion(rdfs:isDefinedBy :FailureCause "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :FailureCause "FailureCause"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :FailureCause "Failure causes are defects in design, process, quality, or part application, which are the underlying cause of a failure or which initiate a process which leads to failure. Also, the failure effect at the highest indenture level or total system. A failure mode may have more causes."@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :FailureCause "(abnormal) vibration input from another (possibly failed) system."@en)
EquivalentClasses(:FailureCause ObjectSomeValuesFrom(:isEndEffect :FailureMode))
SubClassOf(:FailureCause :Cause)
SubClassOf(:FailureCause :FailureMode)

# Class: :FailureEffect (FailureEffect)

AnnotationAssertion(rdfs:isDefinedBy :FailureEffect "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :FailureEffect "FailureEffect"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :FailureEffect "Immediate consequences of a failure on operation, function or functionality, or status of some item."@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :FailureEffect "Increased range of input data."@en)
EquivalentClasses(:FailureEffect ObjectSomeValuesFrom(:hasFailureMode :FailureMode))
SubClassOf(:FailureEffect :Effect)
SubClassOf(:FailureEffect :FailureMode)

# Class: :FailureMode (FailureMode)

AnnotationAssertion(rdfs:isDefinedBy :FailureMode "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :FailureMode "FailureMode"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :FailureMode "The specific manner or way by which a failure occurs in terms of failure of the item (being a part or (sub) system) function under investigation; it may generally describe the way the failure occurs. It shall at least clearly describe a (end) failure state of the item (or function in case of a Functional FMEA) under consideration. It is the result of the failure mechanism (cause of the failure mode).."@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :FailureMode " Too high values."@en)
EquivalentClasses(:FailureMode ObjectSomeValuesFrom(:happenedAt <http://www.w3.org/ns/ssn/System>))
EquivalentClasses(:FailureMode ObjectSomeValuesFrom(:hasCause :FailureCause))
EquivalentClasses(:FailureMode ObjectSomeValuesFrom(:hasEffect :FailureEffect))

# Class: :IntermediateEffect (IntermediateEffect)

AnnotationAssertion(rdfs:isDefinedBy :IntermediateEffect "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :IntermediateEffect "IntermediateEffect"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :IntermediateEffect "The failure effect as it applies at the next higher indenture level."@en)
EquivalentClasses(:IntermediateEffect ObjectSomeValuesFrom(:isIntermediateEffect :FailureMode))
SubClassOf(:IntermediateEffect :FailureEffect)

# Class: :LocalEffect (LocalEffect)

AnnotationAssertion(rdfs:isDefinedBy :LocalEffect "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :LocalEffect "LocalEffect"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :LocalEffect "The visible failure effect as it applies to the item under analysis."@en)
EquivalentClasses(:LocalEffect ObjectSomeValuesFrom(:isLocalEffect :FailureMode))
SubClassOf(:LocalEffect :FailureEffect)
SubClassOf(:LocalEffect <http://www.w3.org/ns/sosa/Observation>)

# Class: :MergedAnomaly (:MergedAnomaly)

SubClassOf(:MergedAnomaly :Anomaly)

# Class: :NotReviewed (:NotReviewed)

SubClassOf(:NotReviewed :Status)

# Class: :Rejected (:Rejected)

SubClassOf(:Rejected :Status)

# Class: :RelabeledAnomaly (:RelabeledAnomaly)

SubClassOf(:RelabeledAnomaly :Anomaly)

# Class: :Status (:Status)

SubClassOf(:Status DataSomeValuesFrom(:hasLabeller rdfs:Literal))
SubClassOf(:Status DataSomeValuesFrom(<http://www.w3.org/ns/sosa/resultTime> xsd:dateTime))

# Class: :UnknownPatternAnomaly (:UnknownPatternAnomaly)

SubClassOf(:UnknownPatternAnomaly :Anomaly)

# Class: :fromObservation (fromObservation)

AnnotationAssertion(rdfs:isDefinedBy :fromObservation "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :fromObservation "fromObservation"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :fromObservation "First observation describing the underfined behaviour"@en)
SubClassOf(:fromObservation <http://www.w3.org/ns/sosa/Observation>)

# Class: :observedProperty (observedProperty)

AnnotationAssertion(rdfs:isDefinedBy :observedProperty "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :observedProperty "observedProperty"@en)
SubClassOf(:observedProperty <http://www.w3.org/ns/sosa/ObservableProperty>)

# Class: :toObservation (toObservation)

AnnotationAssertion(rdfs:isDefinedBy :toObservation "http://IBCNServices.github.io/Folio-Ontology/Folio.owl")
AnnotationAssertion(rdfs:label :toObservation "toObservation"@en)
SubClassOf(:toObservation <http://www.w3.org/ns/sosa/Observation>)


